{
  "name": "functions",
  "scripts": {
    "build": "tsc",
    "lint": "tslint --project tsconfig.json",
    "heroku-postbuild": "yarn run migrate up",
    "migrate": "node-pg-migrate --tsconfig tsconfig.json",
    "migrate-local": "DATABASE_URL=postgres://localhost:5432/postgres node-pg-migrate --tsconfig tsconfig.json",
    "postinstall": "yarn run build",
    "start": "node lib/index.js",
    "start-local": "ts-node src/index.ts"
  },
  "engines": {
    "node": "10"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@slack/bolt": "^2.4.0",
    "@slack/oauth": "^1.2.0",
    "@slack/types": "^1.8.0",
    "@slack/web-api": "^5.12.0",
    "@types/node-schedule": "^1.3.0",
    "@types/request-promise": "^4.1.45",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "flyway": "^0.1.9",
    "fp-ts": "^2.10.5",
    "json5": "^2.1.3",
    "moment": "^2.27.0",
    "node-pg-migrate": "^4.8.0",
    "node-schedule": "^1.3.2",
    "pg": "^7.18.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.2",
    "@types/pg": "^7.14.4",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.5"
  },
  "private": true
}
